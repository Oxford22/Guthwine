{
  "name": "@guthwine/demo",
  "version": "2.0.0",
  "description": "Zero-dependency demo for Guthwine - run locally in under 10 minutes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "guthwine-demo": "./dist/cli.js",
    "guthwine-mcp-demo": "./dist/mcp-server.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js dist/mcp-server.js",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts"
  },
  "dependencies": {
    "@guthwine/core": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ioredis-mock": "^8.9.0",
    "json-logic-js": "^2.0.2",
    "ora": "^8.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/ioredis-mock": "^8.2.6",
    "@types/json-logic-js": "^2.0.7",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "guthwine",
    "ai-agent",
    "authorization",
    "demo",
    "sqlite"
  ],
  "license": "MIT"
}
